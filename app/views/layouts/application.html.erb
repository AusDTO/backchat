<!DOCTYPE html>
<html>
	<%= render 'layouts/head' %>
  <style type="text/css">body {padding-top: 50px;}</style>
	<body>
  <!-- NAV BAR -->
  <%= render 'layouts/partials/dashboard/nav' %>

  <!-- MAIN CONTENT -->
  <div class="container">
    <!-- ALERTS -->

    <div class="row">
      <div class="col-xs-12">
        <% if notice %>
            <p class="alert alert-info"><%= notice %></p>
        <% end %>
        <% if alert %>
            <p class="alert alert-danger"><%= alert %></p>
        <% end %>
      </div>
    </div>
    <!-- MAIN CONTENT -->
    <div class="row">

      <div class="col-sm-12 col-md-10 col-md-offset-1 main">


        <!-- PAGES -->
        <%= yield %>

      </div>

    </div>
  </div>


  </body>
</html>