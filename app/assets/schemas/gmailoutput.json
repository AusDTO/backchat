{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Gmail Output Configuration",
  "type": "object",
  "required": [
    "to","subject"
  ],
  "properties": {
    "to": {
      "type": "string",
      "description": "Email To address"
    },
    "subject": {
      "type": "string",
      "description": "Email Subject"
    },
    "files_allowed": {
      "type": "string",
      "enum": [
        "Yes",
        "No"
      ],
      "description": "Allow file attachments"
    },
    "data_attached": {
      "type": "string",
      "enum": [
        "No data attachment",
        "CSV",
        "JSON"
      ],
      "description": "Attach data as CSV/JSON"
    }

  }
}
